---
import UseCaseTemplate from '../../../components/UseCaseTemplate.astro';
import { getTranslations } from '../../../i18n';

const t = getTranslations('en');
const useCase = t.useCases.gdprEvent;

const metricsLabels = {
	forms: 'Forms',
	timeframe: 'Timeframe',
	processing: 'Integration',
	compliance: 'GDPR Compliant'
};
---

<UseCaseTemplate
	useCase={useCase}
	translations={t}
	locale="en"
	useCaseKey="gdprEvent"
	themeColor="blue"
	breadcrumbTitle="GDPR Event Registration"
	metricsLabels={metricsLabels}
/>
